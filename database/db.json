{
  "base_query":"(collection:inlibrary OR ((openlibrary_edition:* OR sponsor:Google OR collection:digitallibraryindia OR collection:librivoxaudio) AND (mediatype:texts OR mediatype:audio) AND -repub_state:('-2' OR '-1' OR 0 OR 10 OR 11 OR 12 OR 13 OR 18) AND -collection:*test* AND -_exists_:access-restricted-item))",
  "sections":{
    "search": {
      "title":"User search",
      "needs_base_query":false,
      "facets":[
        "languages",
        "categories",
        "availability"
      ],
      "items": {
        "user-search": {
          "title": "Results for query",
          "query": "base_query"
        }
      }
      },
    "collections":{
      "title":"Browse Collections",
      "needs_base_query":false,
      "facets":[
        "languages",
        "categories"
      ],
      "items":{
        "lcp-books":{
          "title":"LCP Books Ready to Borrow",
          "query":"(collection:inlibrary AND lending___is_borrowable:true AND format:lcp)"
        },
        "modern-books":{
          "title":"Modern Books",
          "query":"((lending___is_borrowable:true AND collection:inlibrary) OR ((openlibrary_edition:* OR sponsor:Google OR collection:digitallibraryindia) AND mediatype:texts AND -repub_state:('-2' OR '-1' OR 0 OR 10 OR 11 OR 12 OR 13 OR 18) AND date:[1926 TO *] AND -collection:*test* AND -_exists_:access-restricted-item)) AND ((!_exists_:access-restricted-item) OR lending___available_to_borrow:true)"
        },
        "available-now":{
          "title":"Available Now",
          "query":"((lending___available_to_borrow:true AND lending___is_borrowable:true AND collection:inlibrary) OR ((openlibrary_edition:* OR sponsor:Google OR collection:digitallibraryindia) AND mediatype:texts AND -repub_state:('-2' OR '-1' OR 0 OR 10 OR 11 OR 12 OR 13 OR 18) AND date:[1926 TO *] AND -collection:*test* AND -_exists_:access-restricted-item))"
        },
        "print-disability":{
          "title":"Print Disability Access",
          "query":"collection:printdisabled"
        },
        "audiobooks":{
          "title":"Librivox Audiobooks",
          "query":"collection:librivoxaudio"
        }
      }
    },
